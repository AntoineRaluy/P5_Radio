{% extends 'base.html.twig' %}

{% block body %}
    <h1>Homepage</h1>

    {% for article in articles %}
    <a class="a-title" href="{{ path('app_article_show', { slug: 'article.slug' }) }}"><h2>{{ article.title }}</h2></a>
    <p class="d-inline">{{ article.content |parse_markdown }}</p>

    {% endfor %}
{% endblock %}
